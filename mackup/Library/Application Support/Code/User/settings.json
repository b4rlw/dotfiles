{
    "telemetry.telemetryLevel": "off",
    "editor.renderWhitespace": "none",
    // "workbench.editor.openPositioning": "last",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "jupyter.askForKernelRestart": false,
    "notebook.markup.fontSize": 0,
    "python.languageServer": "Pylance",
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "security.workspace.trust.untrustedFiles": "open",
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "terminal.external.osxExec": "iTerm.app",
    "terminal.integrated.defaultProfile.osx": "zsh",
    "terminal.integrated.profiles.osx": {
        "zsh": {
            "path": "/bin/zsh"
        }
    },
    "terminal.integrated.inheritEnv": false,
    "terminal.integrated.env.osx": {
        "PATH": ""
    },
    "terminal.integrated.fontFamily": "MesloLGS NF",
    "python.defaultInterpreterPath": "/Users/jordan/mambaforge/bin/python",
    "git.ignoreLegacyWarning": true,
    "workbench.sideBar.location": "left",
    // "editor.fontFamily": "Cartograph CF",
    "editor.fontFamily": "MonoLisa",
    // "editor.bracketPairColorization.enabled": false,
    // "editor.fontWeight": "400",
    // "window.zoomLevel": -0.6,
    "editor.lineHeight": 0,
    "editor.fontLigatures": true,
    "notebook.output.textLineLimit": 100,
    "oneDarkPro.italic": true,
    "workbench.colorCustomizations": {
        "[Horizon Extended Blue]": {
            "statusBar.background": "#191b22",
            "statusBar.foreground": "#D5D8DA80",
            // "statusBar.debuggingBackground": "#fe5e78",
            // "statusBar.debuggingForeground": "#06060C",
            // "statusBar.noFolderBackground": "#191b22",
            // "statusBarItem.hoverBackground": "#2E303E",
            "statusBarItem.remoteBackground": "#2A7BCF",
            "statusBarItem.remoteForeground": "#D5D8DA",
            // "statusBarItem.prominentBackground": "#2E303E",
            // "statusBarItem.prominentHoverBackground": "#6C6F93",
            // "titleBar.activeBackground": "#1C1E26",
            // "titleBar.activeBackground": "#191b22",
        },
        // "[Celestial]": {
        //     "editor.background": "#000000"
        // }
    },
    "editor.tokenColorCustomizations": {
        "[Celestial]": {
            "textMateRules": [
                {
                    "name": "Strings",
                    "scope": [
                        "string.quoted",
                        "string.template"
                    ],
                    "settings": {
                        "foreground": "#1e9b6e"
                    }
                },
            ]
        },
    },
    "editor.semanticTokenColorCustomizations": {
        "[Celestial]": {
            "enabled": true,
            "rules": {
                "selfParameter": "#FAC29AE6",
                // "variable": "#BBBBBB",
                // "property": "#BBBBBB",
            }
        },
        "[Horizon Extended]": {
            "enabled": true,
            "rules": {
                "selfParameter": "#FFD486E6",
            }
        },
        "[Horizon Extended Blue]": {
            "enabled": true,
            "rules": {
                "selfParameter": "#FFD486E6",
            }
        }
    },
    "python.formatting.provider": "black",
    "editor.formatOnSave": true,
    "python.terminal.activateEnvironment": true,
    "cSpell.language": "en-GB",
    "jupyter.jupyterServerType": "local",
    "[latex]": {
        "editor.formatOnSave": true,
        "editor.wordWrap": "on",
    },
    "latex-workshop.intellisense.package.enabled": true,
    "latex-workshop.synctex.synctexjs.enabled": true,
    "latex-workshop.synctex.afterBuild.enabled": true,
    "latex-workshop.bibtex-format.sortby": [
        "year",
        "author",
        "title",
    ],
    "latex-workshop.view.pdf.viewer": "tab",
    // "latex-workshop.latex.autoBuild.cleanAndRetry.enabled": true,
    // "latex-workshop.latex.autoClean.run": "onBuilt",
    // "latex-workshop.latex.clean.subfolder.enabled": true,
    // "latex-workshop.latex.clean.fileTypes": [
    //     "*.aux",
    //     "*.bbl",
    //     "*.blg",
    //     "*.idx",
    //     "*.ind",
    //     "*.lof",
    //     "*.lot",
    //     "*.out",
    //     "*.toc",
    //     "*.acn",
    //     "*.acr",
    //     "*.alg",
    //     "*.glg",
    //     "*.glo",
    //     "*.gls",
    //     "*.ist",
    //     "*.fls",
    //     // "*.log",
    //     "*.fdb_latexmk",
    //     "*.snm",
    //     "*.nav",
    //     "*.dvi",
    //     "*.synctex.gz"
    // ],
    "latex-workshop.latex.outDir": "out",
    "latex-workshop.latex.tools": [
        {
            "name": "latexmk",
            "command": "latexmk",
            "args": [
                "-shell-escape",
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-pdf",
                "-aux-directory=out",
                "-output-directory=out",
                "%DOC%"
            ]
        },
        {
            "name": "pdflatex",
            "command": "pdflatex",
            "args": [
                "-shell-escape",
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-aux-directory=out",
                "-output-directory=out",
                "%DOC%"
            ]
        },
        {
            "name": "bibtex",
            "command": "bibtex",
            "args": [
                "%DOCFILE%"
            ]
        }
    ],
    "cSpell.enableFiletypes": [
        "!json"
    ],
    "oneDarkPro.editorTheme": "Just Black",
    "latex-utilities.message.update.show": false,
    // "codesnap.showLineNumbers": true,
    // "codesnap.showWindowControls": false,
    // "codesnap.backgroundColor": "#1c1c1c",
    "codesnap.boxShadow": "rgba(0, 0, 0, 0.65) 20px 20px 20px",
    "editor.fontSize": 13,
    // "editor.fontSize": 14,
    "workbench.iconTheme": "icons",
    "editor.semanticHighlighting.enabled": true,
    "vim.smartRelativeLine": true,
    "python.formatting.blackArgs": [
        "--experimental-string-processing"
    ],
    "terminal.integrated.enableMultiLinePasteWarning": false,
    // "codesnap.showWindowControls": false,
    "vim.useCtrlKeys": true,
    "vim.surround": true,
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "J"
            ],
            "commands": [
                "editor.action.moveLinesDownAction"
            ]
        }, // moveLineDown
        {
            "before": [
                "K"
            ],
            "commands": [
                "editor.action.moveLinesUpAction"
            ]
        }, // moveLineUp
        {
            "before": [
                "u"
            ],
            "commands": [
                "undo"
            ]
        },
        {
            "before": [
                "<C-r>"
            ],
            "commands": [
                "redo"
            ]
        },
    ],
    "editor.suggest.showWords": false,
    "cSpell.enabled": false,
    "Lua.telemetry.enable": false,
    "notebook.lineNumbers": "on",
    "workbench.activityBar.visible": false,
    "workbench.colorTheme": "Celestial Horizon",
    // "editor.scrollbar.horizontal": "hidden",
    // "editor.scrollbar.vertical": "hidden",
    // "editor.overviewRulerBorder": false,
}